{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "CronJobSettings": {
    "StartHour": "05:00",
    "EndHour": "18:00",
    "TaskInterval": "00:20"
  },
  "HttpOrchestratorSettings": {
    "Host": "http://localhost",
    "Port": 5096,
    "Endpoints": [
      {
        "Name": "Status",
        "Path": "/api/cronjob/status"
      }
    ]
  },
  "AllowedHosts": "*"
}
